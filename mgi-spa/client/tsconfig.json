{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2015",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "client": [
        "dist/client",
        "dist/client"
      ],
      "client/*": [
        "dist/client/*",
        "dist/client/*"
      ],
      "client-mock": [
        "dist/client-mock"
      ],
      "client-mock/*": [
        "dist/client-mock/*"
      ],
      "client-model": [
        "dist/client-model"
      ],
      "client-model/*": [
        "dist/client-model/*"
      ]
    }
  }
}